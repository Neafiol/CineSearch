[Unit]
Description=CSbot2
After=multi-user.target


[Service]
WorkingDirectory=/home/projects/CSbot/
User=root

Environment=SERVER="VDS1"
OOMScoreAdjust=-100

ExecStart=/usr/local/bin/python3.7 /home/projects/CStgbot/telsent.py

TimeoutSec=300
Restart=always
RestartSec=2
StartLimitBurst=5